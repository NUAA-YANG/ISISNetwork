    <lxdName>R32</lxdName>
    <interfaces><!--描述接口信息-->
        <interface><!--描述详细接口信息-->
            <name>eth1</name>
            <ipAddress>192.168.15.42</ipAddress>
            <subnetMask>30</subnetMask>
        </interface>
        <interface><!--描述详细接口信息-->
            <name>eth2</name>
            <ipAddress>192.168.15.46</ipAddress>
